@model List<Feedback>

<div class="col-12">
    <table class="table table-dark">
        <thead>
            <tr>
                <th scope="col">#</th>
                <th scope="col">Ad</th>
                <th scope="col">E-poçt</th>
                <th scope="col">Mövzu</th>
                <th scope="col">Mətn</th>
            </tr>
        </thead>
        @{int i = 1; }
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <th scope="row">@i</th>
                    <td>@item.Name</td>
                    <td>@item.Email</td>
                    <td>@item.Theme</td>
                    <td>@item.Context</td>
                </tr>
                i++;
            }
        </tbody>
    </table>
</div>